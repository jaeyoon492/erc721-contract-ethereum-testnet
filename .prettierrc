{
    "printWidth": 80,
    "tabWidth": 2,
    "singleQuote": false,
    "bracketSpacing": true,
    "semi": true,
    "useTabs": false,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "trailingComma": "es5",
    "overrides": [
        {
            "files": "*.sol",
            "options": {
                "bracketSpacing": false,
                "explicitTypes": "preserve"
            }
        }
    ]
}
